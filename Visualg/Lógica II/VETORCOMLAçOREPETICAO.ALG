algoritmo "VetorComLaçoRepeticao"
var

cont: inteiro
alunos : vetor [0..3] de caracter


inicio

 para cont <- 0 ate 3  passo 1 faca
 escreva("Digite o nome do aluno:")
 leia(alunos[cont])
 fimpara
 
 limpatela
 escreval("***Alunos Cadastrados***")
 
  para cont <- 0 ate 3 passo 1 faca
 escreval(alunos[cont])
 fimpara




fimalgoritmo